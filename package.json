{
  
  "husky": {
  
    
  "hooks": {
  
    
    
  "pre-commit": "lint-staged"
  
    
  }
  
  },
  
  "lint-staged": {
  
    
  "*.ts": [
  
    
    
  "eslint --fix",
  
    
    
  "prettier --write"
  
    
  ]
  
  },
  
  "name": "fileguide",
  
  "displayName": "FileGuide",
  
  "description": "Guides developers on what files they need to edit",
  
  "version": "0.0.1",
  
  "engines": {
  
    
  "vscode": "^1.94.0"
  
  },
  
  "categories": [
  
    
  "Other"
  
  ],
  
  "main": "./dist/extension.js",
  
  "contributes": {
  
    
  "activationEvents": [
  
    
    
  "onCommand:extension.openCustomInput"
  
    
  ],
  
    
  "commands": [
  
    
    
  {
  
    
    
    
  "command": "fileguide.openCustomInput",
  
    
    
    
  "title": "Open Custom Input"
  
    
    
  },
  
    
    
  {
  
    
    
    
  "command": "fileguide.analyzeText",
  
    
    
    
  "title": "Analyze Text"
  
    
    
  }
  
    
  ],
  
    
  "capabilities": {
  
    
    
  "untrustedWorkspaces": {
  
    
    
    
  "supported": true
  
    
    
  }
  
    
  },
  
    
  "permissions": [
  
    
    
  "fileSystem"
  
    
  ]
  
  },
  
  "scripts": {
  
    
  "vscode:prepublish": "npm run package",
  
    
  "compile": "npm run check-types && npm run lint && node esbuild.js",
  
    
  "watch": "npm-run-all -p watch:*",
  
    
  "watch:esbuild": "node esbuild.js --watch",
  
    
  "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
  
    
  "package": "npm run check-types && npm run lint && node esbuild.js --production",
  
    
  "compile-tests": "tsc -p . --outDir out",
  
    
  "watch-tests": "tsc -p . -w --outDir out",
  
    
  "pretest": "npm run compile-tests && npm run compile && npm run lint",
  
    
  "check-types": "tsc --noEmit",
  
    
  "lint": "eslint src",
  
    
  "test": "mocha --require ts-node/register src/test/**/*.ts"
  
  },
  
  "devDependencies": {
  
    
  "@eslint/js": "^9.13.0",
  
    
  "@types/mocha": "^10.0.9",
  
    
  "@types/node": "20.x",
  
    
  "@types/vscode": "^1.94.0",
  
    
  "@typescript-eslint/eslint-plugin": "^8.10.0",
  
    
  "@typescript-eslint/parser": "^8.10.0",
  
    
  "@vscode/test-cli": "^0.0.10",
  
    
  "@vscode/test-electron": "^2.4.1",
  
    
  "esbuild": "^0.24.0",
  
    
  "eslint": "^9.13.0",
  
    
  "eslint-config-prettier": "^9.1.0",
  
    
  "eslint-plugin-prettier": "^5.2.1",
  
    
  "globals": "^15.11.0",
  
    
  "husky": "^9.1.6",
  
    
  "lint-staged": "^15.2.10",
  
    
  "mocha": "^10.7.3",
  
    
  "npm-run-all": "^4.1.5",
  
    
  "prettier": "^3.3.3",
  
    
  "typedoc": "^0.26.10",
  
    
  "typescript": "^5.6.2",
  
    
  "typescript-eslint": "^8.10.0"
  
  },
  
  "dependencies": {
  
    
  "node-fetch": "^3.3.2"
  
  }
}
